<% if (theme.plugins.parallax.enable) { %>
  <div id="parallax-window"></div>
<% } else if (theme.plugins.lazyload && theme.plugins.lazyload.enable) { %>
  <div class='cover-bg lazyload placeholder' data-bg="<%- theme.cover.background %>" fetchpriority="low"></div>
<% } else { %>
  <div class='cover-bg' style='background-image:url(<%- theme.cover.background %>)' fetchpriority="low"></div>
<% } %>
<%- partial(theme.cover.layout_scheme) %>